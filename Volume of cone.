#include<stdio.h>

int main(){
    
    int r, h, vol=0;
    float pi=3.14;
    
    printf("enter radius of the cone : ");
    scanf("%d", &r);
    
    printf("enter height of the cone : ");
    scanf("%d", &h);
    
    vol= pi*r*r*h/3;
    printf("Volume of cone is %d", vol);
    
    return 0;
}
