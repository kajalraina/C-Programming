#include<stdio.h>

int main(){
    
    int d1, d2, area=0;
    
     
    printf("enter the value of diagonal : ");
    scanf("%d", &d1);
    
     
    printf("enter the value of 2nd diagonal : ");
    scanf("%d", &d2);
    
    area= d1*d2/2;
    printf("Area of rhombus is %d", area);
    
    return 0;
}
