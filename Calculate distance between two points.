#include<stdio.h>
#include<math.h>

int main(){
    
    float x1, x2, y1, y2, distance=0;
    
    printf("enter first two numbers : (x1, x2)\n");
    scanf("%f%f", &x1, &x2);
    
    printf("enter first two numbers : (y1, y2)\n");
    scanf("%f%f", &y1, &y2);
    
    distance =((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));
    printf("Distance is %f", sqrt(distance));
    
    return 0; 
}
