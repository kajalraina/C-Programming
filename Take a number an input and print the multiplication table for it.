#include<stdio.h>

int main(){
    
    int num ,endNum, i;
    
    
    printf("enter a number : ");
    scanf("%d", &num);
    
    printf("enter an end number : ");
    scanf("%d", &endNum);
    
    
    for(i=1; i<=endNum; i++){
        
        printf("%d\t",i*num);
    }
    
    return 0;
}
