#include<stdio.h>

int main(){
    
    int n1, n2, s, i, n=1, npr=0;
    
    printf("enter total number of things : ");
    scanf("%d", &n1);
    
    printf("enter number of things to be chosen : ");
    scanf("%d", &n2);
    
    s= n1-n2;
    
    for(i=s+1; i<=n1; i++){
        
        n= n*i;
    }
    
    npr= n;
    printf("NPR of given number is %d", npr);
    
    return 0;
}
