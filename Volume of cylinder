#include<stdio.h>

int main(){
    
    int r, h, vol=0;
    float pi=3.14;
    
    printf("enter the radius of cylinder : ");
    scanf("%d", &r);
    
    printf("enter the height of prism : ");
    scanf("%d", &h);
    
    vol= pi*r*r*h;
    printf("Volume of the cylinder is %d",vol);
    
    return 0;
}
